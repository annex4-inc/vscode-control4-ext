{
    "files.associations": {
        "*.c4c": "json"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/intermediate": true,
        "**/node_modules": true,
        "**/output": true
    },
    "Lua.diagnostics.globals": [
        "Properties",
        "JSON"
    ],
    "Lua.workspace.library": [
        ".\\node_modules"
    ],
    "Lua.diagnostics.libraryFiles": "Enable"
}